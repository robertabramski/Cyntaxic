package com.cyntaxic.cynmvc{	import com.cyntaxic.cynmvc.view.CynView;	import com.cyntaxic.cynccess.cynternal;		import flash.display.DisplayObject;	import flash.events.Event;
		/**	 * @private Used only by the framework for notifications.	 * 	 */	public class CyntaxicEvent extends Event	{		use namespace cynternal;				private var _handle:String;		private var _vo:CyntaxicVO;		private var _view:DisplayObject;
 		public static const NOTIFY:String = "notify";				/**		 * @private Used only by the framework for notifications.		 * 		 */				public function CyntaxicEvent(handle:String, vo:CyntaxicVO, view:DisplayObject, bubbles:Boolean = false, cancelable:Boolean = false)		{			_handle = handle;			_vo = vo;			_view = view;						super(NOTIFY, bubbles, cancelable);		}				public function get handle():String { return _handle; }		public function set handle(value:String):void { _handle = value; }		public function get vo():CyntaxicVO { return _vo; }		public function set vo(value:CyntaxicVO):void { _vo = value; }				public function get view():DisplayObject { return _view; }		public function set view(value:DisplayObject):void { _view = value; }				public function describe(compact:Boolean = true):String		{			return Cyntaxic.describe(this, compact);		}	}}